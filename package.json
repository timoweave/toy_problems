    "test": "npm run test_mocha_live",
    "test_jasmine": "node jasmine.test.js",
    "test_mocha": "mocha spec --opts mocha.cmdopts ",
    "test_karma": "karma start ./karma.conf.js",
    "test_phantom": "karma start ./karma.conf.js --browsers PhantomJS",
    "test_jasmine_html": "node jasmine.test.js",
    "start_live_reports": "live-server --open=./reports.html --watch=./reports.html",
    "test_mocha_live": "npm-run-all -p test_mocha start_live_reports",
    "clean_toys": "find ./toys -name '*~' -exec \\rm -rf {} \\;",
    "clean_spec": "find ./spec -name '*~' -exec \\rm -rf {} \\;",
    "clean_backups": "npm run clean_toys & npm run clean_spec"
